@article{Chung2012,
abstract = {Using direct numerical simulation, we investigate stationary and homogeneous shear-driven turbulence in various stratifications, ranging from neutral to very stable. To attain and maintain a stationary flow, we throttle the mean shear so that the net production stays constant for all times. This results in a flow that is characterized solely by its mean shear and its mean buoyancy gradient, independent of initial conditions. The method of throttling is validated by comparison with experimental spectra in the case of neutral stratification. With increasing stratification comes the emergence of vertically sheared large-scale horizontal motions that preclude a straightforward interpretation of flow statistics. However, once these motions are excluded, simply by subtracting the horizontal average, the underlying flow appears amenable to the standard methods of turbulence analysis. It is shown that a direct acknowledgement of the confining influence of the periodic simulation box can lead to a meaningful physical interpretation of the large scales. Once an appropriate confinement scale is identified, many features, including horizontal spectra, flux–gradient relationships and length scales, of stratified sheared turbulence can be readily understood, both qualitatively and quantitatively, in terms of Monin–Obukhov similarity theory. Finally, the similarity-theory framework is used to interpret the scaling of the vertical diapycnal diffusivity in stratified turbulence.},
author = {Chung, D and Matheou, G},
doi = {DOI: 10.1017/jfm.2012.59},
edition = {2012/03/01},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
keywords = {ocean processes,stratified turbulence,turbulence simulation},
pages = {434--467},
publisher = {Cambridge University Press},
title = {{Direct numerical simulation of stationary homogeneous stratified sheared turbulence}},
url = {https://www.cambridge.org/core/article/direct-numerical-simulation-of-stationary-homogeneous-stratified-sheared-turbulence/4839B4538EC395646BD650A5183BFF14},
volume = {696},
