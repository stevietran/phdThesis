issn = {0304-3894},
journal = {Journal of hazardous materials},
keywords = {Accidents,Fires,Fossil Fuels,Gravitation,Hot Temperature,Industrial accidents,Model validation,Models,Occupational,Pool fire,Simulation of liquid spreading,Statistical,Surface Properties,Theoretical,Time Factors,Volatilization,Water},
month = {jan},
