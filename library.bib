file = {:home/stevie/Documents/MendeleyFiles/Annual Review of Fluid Mechanics/Cant, Dawes, Savill/Cant, Dawes, Savill - 2004 - ADVANCED CFD AND MODELING OF ACCIDENTAL EXPLOSIONS.pdf:pdf},
journal = {Annual Review of Fluid Mechanics},
number = {1},
pages = {97--119},
title = {{ADVANCED CFD AND MODELING OF ACCIDENTAL EXPLOSIONS}},
