doi = {10.1016/j.ijheatfluidflow.2014.10.018},
file = {:home/stevie/Documents/MendeleyFiles/International Journal of Heat and Fluid Flow/Lal, Sato, Niceno/Lal, Sato, Niceno - 2015 - Direct numerical simulation of bubble dynamics in subcooled and near-saturated convective nucleate boiling.pdf:pdf},
