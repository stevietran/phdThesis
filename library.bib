doi = {10.1016/j.jhazmat.2012.01.007},
file = {:home/stevie/Documents/MendeleyFiles/Journal of Hazardous Materials/Siddiqui, Jayanti, Swaminathan/Siddiqui, Jayanti, Swaminathan - 2012 - CFD analysis of dense gas dispersion in indoor environment for risk assessment and risk mitigati.pdf:pdf},
issn = {03043894},
journal = {Journal of Hazardous Materials},
keywords = {CFD,Dense gas dispersion,Indoor environment,Risk assessment,Risk mitigation,Species transport method},
month = {mar},
pages = {177--185},
pmid = {22301078},
title = {{CFD analysis of dense gas dispersion in indoor environment for risk assessment and risk mitigation}},
url = {http://www.sciencedirect.com/science/article/pii/S0304389412000234},
volume = {209-210},
year = {2012}
}
