keywords = {dense gas,dispersion,experimental data,model evaluation},
month = {sep},
number = {5},
pages = {323--338},
title = {{An evaluation of validation procedures and test parameters for dense gas dispersion models}},
url = {http://www.sciencedirect.com/science/article/pii/0950423096000277},
