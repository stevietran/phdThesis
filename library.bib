journal = {Process Safety and Environmental Protection},
keywords = {CFD,atmospheric boundary layer,complex terrain,dense cloud dispersion,heavy gas},
month = {may},
number = {3},
pages = {242--256},
title = {{Heavy Gas Dispersion Modelling Over a Topographically Complex Mesoscale}},
url = {http://www.sciencedirect.com/science/article/pii/S095758200571243X},
volume = {83},
year = {2005}
}
