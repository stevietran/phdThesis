@incollection{Rota2016,
abstract = {Both fires and explosions are originated by the release of a given amount of energy in the environment. However, while fires involve a combustion reaction between a fuel and an oxidant that releases the combustion energy slowly enough, explosions release the energy stored in the system in a time and in a volume small enough to generate a pressure wave. In this view, fires are characterized by flames and heat radiation, while explosions by pressure waves. This article reports a brief classification of the main fires and explosions found in process industry accidents, as well as a short discussion on the main features and on the simplest models available to estimate the effects of industrial fires (ie, pool fires, jet fires, fireballs, and flash fires) and explosions (ie, condensed phase explosions, vapor cloud explosions, boiling liquid expanding vapor explosions, rapid phase transitions, and pressure vessel bursting).},
author = {Rota, R.},
booktitle = {Reference Module in Chemistry, Molecular Sciences and Chemical Engineering},
doi = {10.1016/B978-0-12-409547-2.11177-1},
