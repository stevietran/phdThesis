issn = {1873-3336},
journal = {Journal of hazardous materials},
keywords = {Chemical Hazard Release,Chemical Hazard Release: statistics {\&} numerical da,Gases,Gases: toxicity,Hazardous Substances,Models, Theoretical,Urban Health,Urban Population},
month = {apr},
