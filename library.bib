file = {:home/stevie/Documents/MendeleyFiles/Reliability Engineering {\&} System Safety/Paltrinieri, Tugnoli, Cozzani/Paltrinieri, Tugnoli, Cozzani - 2015 - Hazard identification for innovative LNG regasification technologies.pdf:pdf},
issn = {09518320},
