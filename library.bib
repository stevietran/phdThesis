doi = {10.1016/j.ces.2005.08.042},
file = {:home/stevie/Documents/MendeleyFiles/Chemical Engineering Science/Sklavounos, Rigas/Sklavounos, Rigas - 2006 - Simulation of Coyote series trials—Part I.pdf:pdf},
