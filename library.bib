keywords = {Cloud fire,Combustion,Coyote trials,Eddy dissipation model,Flash fire,Ignition,P1 model,Thermal radiation},
month = {mar},
number = {5},
pages = {1444--1452},
title = {{Simulation of Coyote series trials—Part II: A computational approach to ignition and combustion of flammable vapor clouds}},
volume = {61},
year = {2006}
