@article{Middha2010,
abstract = {Computational fluid dynamics (CFD) tools are increasingly employed for quantifying incident consequences in quantitative risk analysis (QRA) calculations in the process industry. However, these tools must be validated against representative experimental data, involving combined release and ignition scenarios, in order to have a real predictive capability. Forschungszentrum Karlsruhe (FZK) has recently carried out experiments involving vertically upwards hydrogen releases with different release rates and velocities impinging on a plate in two different geometrical configurations. The dispersed cloud was subsequently ignited and resulting explosion overpressures recorded. Blind CFD simulations were carried out prior to the experiments to predict the results. The simulated gas concentrations are found to correlate reasonably well with observations. The overpressures subsequent to ignition obtained in the blind predictions could not be compared directly as the ignition points chosen in the experiments were somewhat different from those used in the blind simulations, but the pressure levels were similar. Simulations carried out subsequently with the same ignition position as those in the experiments compared reasonably well with the observations. This agreement points to the ability of the CFD tool FLACS to model such complex scenarios even with hydrogen as a fuel. Nevertheless, the experimental set-up can be considered to be small-scale. Future large-scale data of this type will be valuable to confirm ability to predict large-scale accident scenarios.},
author = {Middha, Prankul and Hansen, Olav R and Grune, Joachim and Kotchourko, Alexei},
doi = {http://dx.doi.org/10.1016/j.jhazmat.2010.02.061},
file = {:home/stevie/Documents/MendeleyFiles/Journal of Hazardous Materials/Middha et al/Middha et al. - 2010 - CFD calculations of gas leak dispersion and subsequent gas explosions Validation against ignited impinging hydrog.pdf:pdf},
isbn = {0304-3894},
journal = {Journal of Hazardous Materials},
keywords = {CFD,Dispersion,Explosion,Hydrogen,Impinging jets,Risk analysis},
number = {1–3},
pages = {84--94},
title = {{CFD calculations of gas leak dispersion and subsequent gas explosions: Validation against ignited impinging hydrogen jet experiments}},
url = {http://www.sciencedirect.com/science/article/pii/S030438941000275X},
volume = {179},
year = {2010}
}
